lista comandi

ESPRESSIONI REGOLARI

epressione regolare: python3 src/main.py -t 0 --json data/espressione_regolare.json

epressioni regolari: python3 src/main.py -t 1 --json data/espressioni_regolari.json


PRIMA RETE DI FA

spazio_comportamentale: python3 src/main.py -t 2 --json data/fa.json data/transition.json data/original_link.json

spazio_comportamentale_osservabile: python3 src/main.py -t 3 --json data/fa.json data/transition.json data/original_link.json -O "o3,o2"

diagnosi_relativa_osservazione: python3 src/main.py -t 4 --json data/fa.json data/transition.json data/original_link.json -O "o3,o2"

diagnostica: python3 src/main.py -t 5 --json data/fa.json data/transition.json data/original_link.json

diagnosi_lineare: python3 src/main.py -t 6 --json data/fa.json data/transition.json data/original_link.json -O "o3,o2"

diagnosi_relativa_osservazione_from_spazio_comportamentale_osservabile: python3 src/main.py -t 7 --bin logs_prima_rete_FA/serialized_objects/obs_behave_state -O "o3,o2"

diagnostica_from_spazio_comportamentale: python3 src/main.py -t 8 --bin logs_prima_rete_FA/serialized_objects/behave_state

diagnostica_from_silent_closure_space: python3 src/main.py -t 9 --bin logs_prima_rete_FA/serialized_objects/silent_closure_space

diagnosi_lineare_from_spazio_comportamentale: python3 src/main.py -t 10 --bin logs_prima_rete_FA/serialized_objects/behave_state -O "o3,o2"

diagnosi_lineare_from_silent_closure_space: python3 src/main.py -t 11 --bin logs_prima_rete_FA/serialized_objects/silent_closure_space -O "o3,o2"

diagnosi_lineare_from_diagnostic_graph: python3 src/main.py -t 12 --bin logs_prima_rete_FA/serialized_objects/diagnostic_graph -O "o3,o2"


